<h1>Books!!!</h1>

<% @books.each do |book| %>
  <tr>
    <td><%= link_to "#{book.title}", book %></td>
    <td><%= book.description %></td>
    <td><%= book.price %></td>
    <td><%= link_to 'Add to Cart', book, class: "btn btn-primary" %></td>
    <td><%= link_to(image_tag("https://www.paypal.com/en_US/i/btn/btn_xpressCheckout.gif"), express_checkout_path) %><td>
  </tr>
<% end %>